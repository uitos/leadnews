<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.heima.wemedia.mapper.WmMaterialMapper">


    <select id="selectBatchUrls" resultType="com.heima.model.wm.pojos.WmMaterial"
            parameterType="java.util.List">
        select id,url,type from wm_material where url in
        <foreach collection="list" item="url" open="(" separator="," close=")">
            #{url}
        </foreach>
    </select>
</mapper>
